{
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "no-console": "off",
    "func-names": "off",
    "eqeqeq": "off",
    "consistent-return": "off",
    "no-unused-vars": "off",
    "no-useless-escape": "off"
  }
}
